<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - Quan_ly_ho_so_nhan_su</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.css" />
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/Quan_ly_ho_so_nhan_su.styles.css" asp-append-version="true" />
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.11.3/font/bootstrap-icons.min.css">
    <link rel="stylesheet" href="//cdn.datatables.net/2.0.8/css/dataTables.dataTables.min.css" />
    <link rel="stylesheet" href="//cdnjs.cloudflare.com/ajax/libs/toastr.js/latest/toastr.min.css">
</head>
<body>
    <header>
        <nav class="navbar navbar-expand-sm navbar-toggleable-sm navbar-light bg-primary border-bottom box-shadow mb-3">
            <div class="container-fluid">
                <a class="navbar-brand text-white-50" asp-area="" asp-controller="Home" asp-action="Index">VMO</a>
                <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target=".navbar-collapse" aria-controls="navbarSupportedContent"
                        aria-expanded="false" aria-label="Toggle navigation">
                    <span class="navbar-toggler-icon"></span>
                </button>
                <div class="navbar-collapse collapse d-sm-inline-flex justify-content-between">
                    <ul class="navbar-nav flex-grow-1">
                        <li class="nav-item">
                            <a class="nav-link text-white-50" asp-area="" asp-controller="Home" asp-action="Index">Home</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link text-white-50" asp-area="" asp-controller="Home" asp-action="Privacy">Privacy</a>
                        </li>
                        <li class="nav-item dropdown">
                            <a class="nav-link dropdown-toggle text-white-50" data-bs-auto-close="outside" href="#" role="button" data-bs-toggle="dropdown" aria-expanded="false">
                                Quản lý danh mục
                            </a>
                            <ul class="dropdown-menu">
                                <li class="nav-item">
                                    <a class="dropdown-item" asp-controller="ChucDanh" asp-action="Index">Chức danh</a>
                                </li>
                                <li class="nav-item">
                                    <a class="dropdown-item" asp-controller="HopDong" asp-action="Index">Hợp đồng</a>
                                </li>
                                <li class="dropend">
                                    <a class="nav-link dropdown-toggle text-muted px-3" href="#" role="button" data-bs-toggle="dropdown" aria-expanded="false">
                                        Danh mục tỉnh thành
                                    </a>
                                    <ul class="dropdown-menu">
                                        <li class="nav-item">
                                            <a class="dropdown-item" asp-controller="QuocGia" asp-action="Index">Quốc gia</a>
                                        </li>
                                        <li class="nav-item">
                                            <a class="dropdown-item" asp-controller="TinhThanh" asp-action="Index">Tỉnh thành</a>
                                        </li>
                                        <li class="nav-item">
                                            <a class="dropdown-item" asp-controller="QuanHuyen" asp-action="Index">Quận Huyện</a>
                                        </li>
                                        <li class="nav-item">
                                            <a class="dropdown-item" asp-controller="XaPhuong" asp-action="Index">Xã Phường</a>
                                        </li>
                                    </ul>
                                </li>
                            </ul>
                        </li>
                    </ul>
                </div>
            </div>
        </nav>
    </header>
    <div class="container">
        <main role="main" class="pb-5">
            <partial name="_Notification"></partial>
            @RenderBody()
        </main>
    </div>

    <footer class="border-top footer bg-primary">
        <div class="container text-white-50">
            <i class="bi bi-c-circle"></i> 2024 - Quản lý hồ sơ nhân sự by Noaaaaa
        </div>
    </footer>
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    <script src="https://cdn.tiny.cloud/1/cqevn3af2wbdm006e845qeom5phj7ialv0kme9lk8e870bc8/tinymce/7/tinymce.min.js" referrerpolicy="origin"></script>
    <script src="//cdn.datatables.net/2.0.8/js/dataTables.min.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/sweetalert2@11"></script>
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
